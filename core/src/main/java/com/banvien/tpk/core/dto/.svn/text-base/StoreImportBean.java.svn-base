public class StoreImportBean extends AbstractBean<StoreImportDTO> {

    private List<ProductImportDTO> productImports;

    public List<ProductImportDTO> getProductImports() {
        if (productImports == null) {
            productImports = new ArrayList<ProductImportDTO>();
        }
        return productImports;
    }

    public void setProductImports(List<ProductImportDTO> productImports) {
        this.productImports = productImports;
    }
}
