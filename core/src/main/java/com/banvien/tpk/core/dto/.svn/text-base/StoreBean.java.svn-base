public class StoreBean extends AbstractBean<Store> {
    private List<Storesosmaster> storeSOSMasters;
    private Long[] registeredProducts;

    public StoreBean() {
        this.pojo = new Store();
    }

    public List<Storesosmaster> getStoreSOSMasters() {
        return storeSOSMasters;
    }

    public void setStoreSOSMasters(List<Storesosmaster> storeSOSMasters) {
        this.storeSOSMasters = storeSOSMasters;
    }

    public Long[] getRegisteredProducts() {
        return registeredProducts;
    }

    public void setRegisteredProducts(Long[] registeredProducts) {
        this.registeredProducts = registeredProducts;
    }
}
