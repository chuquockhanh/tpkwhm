public class BrandImportDTO implements Serializable {
    private String brand;

    private Integer startColumn;

    private Integer endColumn;

    private Brandgroup brandgroup;


    private BrandGroupImportDTO brandGroupImportDTO;

    public String getBrand() {
        return brand;
    }

    public void setBrand(String brand) {
        this.brand = brand;
    }

    public Integer getStartColumn() {
        return startColumn;
    }

    public void setStartColumn(Integer startColumn) {
        this.startColumn = startColumn;
    }

    public Integer getEndColumn() {
        return endColumn;
    }

    public void setEndColumn(Integer endColumn) {
        this.endColumn = endColumn;
    }

    public BrandGroupImportDTO getBrandGroupImportDTO() {
        return brandGroupImportDTO;
    }

    public void setBrandGroupImportDTO(BrandGroupImportDTO brandGroupImportDTO) {
        this.brandGroupImportDTO = brandGroupImportDTO;
    }

    public Brandgroup getBrandgroup() {
        return brandgroup;
    }

    public void setBrandgroup(Brandgroup brandgroup) {
        this.brandgroup = brandgroup;
    }
}
