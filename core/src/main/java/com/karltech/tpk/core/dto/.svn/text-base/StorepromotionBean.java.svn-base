public class StorepromotionBean extends AbstractBean<Storepromotion> {
    private Long[] scopes;

    private Long[] storeProductsToHandheld;

    public StorepromotionBean() {
        this.pojo = new Storepromotion();

    }

    public Long[] getScopes() {
        return scopes;
    }

    public void setScopes(Long[] scopes) {
        this.scopes = scopes;
    }

	public Long[] getStoreProductsToHandheld() {
		return storeProductsToHandheld;
	}

	public void setStoreProductsToHandheld(Long[] storeProductsToHandheld) {
		this.storeProductsToHandheld = storeProductsToHandheld;
	}

}
